{
  "name": "@gurban/kit",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": [
      "./src/*.ts"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsup src/index.ts --format cjs,esm --dts",
    "test": "yarn jest",
    "lint": "yarn eslint \"src/**/*.{ts,tsx}\" --fix",
    "coverage": "yarn test --coverage",
    "docs:md": "(yarn typedoc --plugin typedoc-plugin-markdown --entryPoints src/index.ts --tsconfig tsconfig.json --excludeNotDocumented --hideBreadcrumbs --hideGenerator --hidePageHeader --searchInComments --searchInDocuments --useCodeBlocks --useCustomAnchors --flattenOutputFiles --out .temp-docs) && (yarn tsx src/bin/clean-md.ts ../kit-wiki/Home.md)"
  },
  "dependencies": {
    "@eslint/js": "^9.38.0",
    "@loancrate/prisma-schema-parser": "^3.0.0",
    "@mrleebo/prisma-ast": "^0.12.1",
    "@swc/helpers": "^0.5.17",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.1"
  },
  "devDependencies": {
    "@tsd/typescript": "^5.9.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.7.2",
    "@yarnpkg/pnpify": "^4.1.6",
    "ajv": "^8.17.1",
    "concat-md": "^0.5.1",
    "documentation": "^14.0.3",
    "globals": "^16.2.0",
    "jest": "^30.2.0",
    "jest-html-reporters": "^3.1.7",
    "jest-runner-tsd": "^6.0.0",
    "jest-util": "^30.2.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 12"
  },
  "peerDependencies": {
    "@nestjs/common": "*",
    "@nestjs/graphql": "*",
    "clsx": "*"
  }
}
