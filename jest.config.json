{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "reporters": [
    "default",
    ["jest-html-reporters", {
      "publicPath": "./coverage/html-report",
      "filename": "report.html",
      "expand": true
    }]
  ],
  "testMatch": [
    "**/*.spec.ts"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/src/unsorted/"
  ]
}
