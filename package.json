    ".": "./dist/src/core/index.js",
    "./graphql": "./dist/src/graphql/index.js",
    "./helpers/eslint-parts.mjs": "./dist/src/helpers/eslint-parts.mjs",
    "./helpers/eslint-parts-front.mjs": "./dist/src/helpers/eslint-parts-front.mjs",
