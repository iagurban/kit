{
  "name": "@gurban/kit",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": [
      "./src/*.ts"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsup src/index.ts --format cjs,esm --dts",
    "test": "yarn jest",
    "lint": "yarn eslint \"src/**/*.{ts,tsx}\" --fix",
    "coverage": "yarn test --coverage",
    "docs:md": "(yarn typedoc --plugin typedoc-plugin-markdown --entryPoints src/index.ts --tsconfig tsconfig.json --excludeNotDocumented --out .temp-docs) && (yarn concat-md .temp-docs > docs/API.md)"
  },
  "dependencies": {
    "@eslint/js": "^9.29.0",
    "@mrleebo/prisma-ast": "^0.12.1",
    "@swc/helpers": "^0.5.17",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lodash": "^4.17.21",
    "prettier": "^3.6.1",
    "typescript-eslint": "^8.35.0"
  },
  "devDependencies": {
    "@tsd/typescript": "^5.8.3",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.17.18",
    "@types/node": "^24.0.4",
    "@yarnpkg/pnpify": "^4.1.5",
    "ajv": "^8.12.0",
    "concat-md": "^0.5.1",
    "documentation": "^14.0.3",
    "globals": "^16.2.0",
    "jest": "^30.0.3",
    "jest-html-reporters": "^3.1.7",
    "jest-runner-tsd": "^6.0.0",
    "jest-util": "^30.0.2",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">= 12"
  }
}
