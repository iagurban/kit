    "./prisma": "./dist/src/prisma/index.js",
    "./react": "./dist/src/react/index.js",
    "./react/mobx": "./dist/src/react/mobx/index.js",
    "./redis": "./dist/src/redis/index.js",
    "./zod": "./dist/src/zod/index.js",
    "./*": "./dist/src/core/*"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "build-info-gen": "./dist/src/bin/build-info-gen.js",
    "clean-md": "./dist/src/bin/clean-md.js",
    "dotenv-flat": "./dist/src/bin/dotenv-flat.js",
    "fetch-unicode-letters-ranges": "./dist/src/bin/fetch-unicode-letters-ranges.js",
    "fix-graphql-codegen": "./dist/src/bin/fix-graphql-codegen.js",
    "watch-n-run": "./dist/src/bin/watch-n-run.js"
  },
  "scripts": {
    "build": "yarn swc-ts-build -s src -d dist/src -p tsconfig.build.json && cp -r ./src/helpers/ ./dist/src && cp -R ./dist/_declarations/* ./dist/ && rm -rf ./dist/_declarations",
    "test": "yarn jest",
    "lint": "yarn eslint \"src/**/*.{ts,tsx}\" --fix",
    "coverage": "yarn test --coverage",
    "docs:md": "(yarn typedoc --plugin typedoc-plugin-markdown --entryPoints src/core/index.ts --tsconfig tsconfig.json --excludeNotDocumented --hideBreadcrumbs --hideGenerator --hidePageHeader --searchInComments --searchInDocuments --useCodeBlocks --useCustomAnchors --flattenOutputFiles --out .temp-docs) && (yarn tsx src/bin/clean-md.ts ../kit-wiki/Home.md)"
  },
