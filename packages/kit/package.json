{
  "name": "@freyja/kit",
  "version": "0.0.1",
  "dependencies": {
    "@swc/helpers": "^0.5.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.9",
    "@eslint/js": "^8.56.0",
    "@next/font": "*",
    "@parcel/core": "^2.14.4",
    "@parcel/packager-ts": "2.13.2",
    "@parcel/transformer-typescript-types": "2.13.2",
    "@tsd/typescript": "^5.3.3",
    "@types/ajv": "^1.0.0",
    "@types/eslint__js": "^8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "ajv": "^8.12.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql": "^16.8.1",
    "ioredis": "*",
    "jest": "^29.7.0",
    "jest-runner-tsd": "^6.0.0",
    "mobx": "^6.12.0",
    "nodemon": "^3.0.2",
    "parcel": "^2.13.2",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@apollo/client": "*",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/inter": "^5",
    "@mui/joy": "*",
    "graphql": "*",
    "ioredis": "*",
    "mobx": "^6.12.0",
    "mobx-keystone": "^1.6",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "setup": "yarn paaoo:run parcel",
    "clean-dist": "rimraf ./.parcel-cache ./dist ./dist-split",
    "build-bundle": "parcel build --cache-dir=./.parcel-cache",
    "watch-bundle": "parcel watch --cache-dir=./.parcel-cache",
    "build-split": "tsc --project tsconfig.build.json",
    "watch-split": "yarn build-split --watch",
    "build": "sh -c \"yarn build-bundle & yarn build-split & wait\"",
    "watch": "sh -c \"yarn watch-bundle & yarn watch-split & wait\"",
    "kit:dev": "yarn watch",
    "kit:prod": "yarn clean-dist && yarn build"
  },
  "engines": {
    "node": ">= 12"
  },
  "main": "dist/main.js",
  "module": "dist/module.js",
  "source": "src/index.ts",
  "targets": {
    "default": {
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  },
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "require": "./dist/main.js",
      "types": "./dist/types.d.ts",
      "default": "./dist/module.js"
    },
    "./ts/*": "./src/*",
    "./js/*": "./dist-split/*"
  }
}
