{
  "name": "@focalm/ui",
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "ui:start-dev": "vite",
    "check-types": "yarn node --max-old-space-size=1024 $(yarn bin tsc) -p tsconfig.json --noEmit",
    "check-types-watch": "yarn check-types --watch",
    "check-types-watch-ci": "sh -c \"yarn check-types-watch --preserveWatchOutput | ../core/src/bin/filter-empty-lines.sh\"",
    "eslint-generated": "yarn node --max-old-space-size=512 $(yarn bin eslint) --rule '@typescript-eslint/no-explicit-any: off' --fix",
    "generate:graphql": "graphql-codegen --config ./codegen.ts && shx mv ./graphql/*.ts ./src/graphql.generated/ && yarn eslint-generated ./src/graphql.generated/**",
    "generate:graphql-watch": "nodemon -d 5 -e 'graphql' -w './graphql/**/*' -w '../server/schema.autogenerated.graphql' -x 'yarn generate:graphql'",
    "i18n:generate": "yarn typesafe-i18n",
    "ui:full-dev": "yarn node --max-old-space-size=2048 $(yarn bin concurrently) -n vite,types,codegen -c cyan,magenta,yellow \"sleep 5 && yarn ui:start-dev\" \"sleep 5 && yarn check-types-watch-ci\" \"yarn generate:graphql-watch | yarn strip-ansi\"",
    "test": "yarn jest",
    "test-watch": "yarn test --watch",
    "test-coverage": "yarn test --coverage",
    "clean-cache": "rimraf ./.vite-cache && rimraf ./dist && rimraf ./node_modules",
    "lint": "yarn eslint \"src/**/*.{ts,tsx}\" --fix"
  },
