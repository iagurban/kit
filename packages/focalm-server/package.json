    "@yarnpkg/pnpify": "^4.1.4",
    "nestjs-prisma-graphql-crud-gen": "1.0.4",
    "prisma": "6.6.0",
    "prisma-class-generator": "^0.2.11",
    "prisma-erd-generator": "^2.0.4",
    "prisma-generator-typescript-interfaces": "^2.1.0",
    "prisma-joi-generator": "^0.2.0",
    "prisma-json-types-generator": "^3.2.3",
    "prisma-nestjs-graphql": "^21.2.0",
    "puppeteer": "^24.6.1",
    "rimraf": "^6.0.1"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "8.32.1"
  }
}
