{
  "name": "@focalm/server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Igor Gurban <iagurban@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "fc-server:start-dev": "nodemon -e 'js,ts,mjs,md,jsx,tsx,yml,yaml,json,html,htm' -w './**/*.ts' -i '**/*.spec.ts' -x 'dotenv ts-node src/main.ts'",
    "dev-hmr": "PATH=\"$HOME/.npm-global/bin:$PATH\" nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "after-client-generation": "yarn rimraf src/generated/old-client && prisma/bin/fix-prisma-imports.sh && ts-node prisma/bin/fix-nestgraphql.ts && yarn eslint --quiet --no-error-on-unmatched-pattern --rule '@typescript-eslint/no-empty-object-type: off' --rule '@typescript-eslint/no-explicit-any: off' --rule '@typescript-eslint/no-unnecessary-type-constraint: off' --rule '@typescript-eslint/no-unsafe-function-type: off' --rule '@typescript-eslint/no-namespace: off' --fix src/generated/**/*",
    "fc-prisma:generate-client": "NODE_ENV=production yarn pnpify prisma generate && yarn after-client-generation",
    "fc-server:prisma-dev": "nodemon -e 'prisma' -w './prisma/schema.prisma' -x 'yarn prisma:generate-client'",
    "fc-prisma:migrate": "yarn pnpify prisma migrate dev && yarn after-client-generation",
    "fc-prisma:migrate-reset": "yarn pnpify prisma migrate reset && yarn after-client-generation",
    "fc-prisma:seed": "yarn prisma db seed"
  },
  "prisma": {
    "seed": "yarn ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@eslint/js": "^9.24.0",
    "@freyja/kit": "workspace:*",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.0.17",
    "@nestjs/core": "^11.0.17",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.17",
    "@nestjs/testing": "^10.4.17",
    "@prisma/client": "6.6.0",
    "@types/bcrypt": "^5",
    "@types/node": "^22.14.1",
    "@types/passport": "^0",
    "@types/passport-local": "^1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@typescript-eslint/typescript-estree": "^8.32.1",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "cookie-parser": "^1.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "globals": "^16.0.0",
    "graphql": "^16.10.0",
    "graphql-type-json": "^0.3.2",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.2",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pnp-webpack-plugin": "^1.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma-graphql-type-decimal": "^3.0.1",
    "prisma-zod-generator": "^0.8.13",
    "react": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "^0.2.3",
    "rxjs": "^7.8.2",
    "supertest": "^7.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "uuid": "^11.1.0",
    "webpack": "^5.99.5",
    "webpack-pnp-externals": "^1.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "11.3.0",
    "@types/cookie-parser": "^1",
    "@types/express": "^5",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1",
