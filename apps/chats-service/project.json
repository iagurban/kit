{
  "name": "chats-service",
  "root": "apps/chats-service",
  "sourceRoot": "apps/chats-service/src",
  "projectType": "application",
  "targets": {
    "clean-dist": {},
    "copy-assets": {},
    "build-service": {},
    "run-service": {
      "configurations": {
        "watch": {
          "watch": [
            "./dist/**/*",
            "../../libs/kit/dist/src/**/*",
            "../../libs/util/dist/src/**/*",
            "../../libs/database/dist/src/**/*",
            "../../apps/signing-service/dist/src/**/*",
            "../../apps/messages-service/dist/src/**/*"
          ]
        }
      }
    },
    "serve-backend": {},
    "issue-service-cert": {},
    "generate-proto-code": {
      "options": {
        "protoFileName": "chats"
      }
    },

    "build": {
      "executor": "nx:noop",
      "dependsOn": ["build-service-ts", "copy-assets"]
    },

    "serve": {
      "executor": "nx:noop",
      "dependsOn": ["serve-backend"]
    }
  },
  "tags": []
}
